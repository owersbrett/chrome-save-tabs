{
  "manifest_version": 2,

  "name": "Save Tabs",
  "description": "Save your opened tabs as json or plain text",
  "version": "0.2.0",

  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage"
  ]
}
